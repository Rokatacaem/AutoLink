{
    "functions": {
        "api/*.py": {
            "memory": 1024,
            "maxDuration": 10
        },
        "app/main.py": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/health",
            "destination": "/api/health.py"
        },
        {
            "source": "/(.*)",
            "destination": "/app/main.py"
        }
    ]
}